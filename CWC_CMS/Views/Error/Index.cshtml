
@{
    Layout = null;
}
 
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Unexpected Error</title>
    <link href="https://fonts.googleapis.com/css?family=Encode+Sans+Semi+Condensed:100,200,300,400" rel="stylesheet">
    <link href="~/css/Error.css" rel="stylesheet" />
</head>

<body class="loading">
    <div class="error-502__container">
        <div class="error-502">
            <div class="error-502__text">
                <h1>Unexpected Error <b>:(</b></h1>
                <h2><p>You can go back to our <a href="/HomePage">Homepage</a>.</p></h2>
            </div>
        </div>
    </div>
    <div class="gears">
        <div class="gear one">
            <div class="bar"></div>
            <div class="bar"></div>
            <div class="bar"></div>
        </div>
        <div class="gear two">
            <div class="bar"></div>
            <div class="bar"></div>
            <div class="bar"></div>
        </div>
        <div class="gear three">
            <div class="bar"></div>
            <div class="bar"></div>
            <div class="bar"></div>
        </div>
    </div>
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script>
        $(function () {
            setTimeout(function () {
                $('body').removeClass('loading');
            }, 1000);
        });
    </script>
</body>
</html>

