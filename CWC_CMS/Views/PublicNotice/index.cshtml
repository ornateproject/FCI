                                    @using (Html.BeginForm("Create", "PublicNotice", FormMethod.Get, new { @class = "form-horizontal", id = "basicForm" }))
                                    {
                                        <div style="width:90%; margin-left:5%;">
                                            <table class="table table-striped table-primary hover" id="datatablelist">
                                                <thead>
                                                    <tr>
                                                        <th width="10%" height="48" align="center" style="background-color:#ef4815; color:white">Sr.No </th>
                                                        <th width="15%">
                                                            Date
                                                        </th>
                                                        <th width="15%">
                                                            Description
                                                        </th>
                                                        <th width="10%">
                                                            English Document
                                                        </th>
                                                        <th width="40%">
                                                            Hindi Document
                                                        </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @if (Model != null)
                                                    {
                                                        foreach (var item in Model)
                                                        {
                                                            <tr>
                                                                <td>
                                                                    @(Rownumber += 1)

                                                                </td>
                                                                <td>
                                                                    <p>@item.NoticeDate.ToString("dd-MMM-yyyy")</p>
                                                                    @*@Html.DisplayFor(modelItem => item.NoticeDate)*@
                                                                </td>

                                                                <td>
                                                                    @Html.DisplayFor(modelItem => item.Description)

                                                                </td>
                                                                <td>
                                                                    @if (item.EnglishDocumentPath != "NA")
                                                                    {
                                                                        @Html.ActionLink("View Document", "Document_View", "PublicNotice", new { FilePath = item.EnglishDocumentPath, noticeid = item.publicNotiecsId, filecheck = 0 }, new { @class = "text-primary ", target = "_blank", style = "width:100px;" })
                                                                    }
                                                                    else
                                                                    {
                                                                        <p class="text-danger">No Document</p>
                                                                    }
                                                                </td>
                                                                <td>
                                                                    @if (item.HindiDocumentPath != "NA")
                                                                    {
                                                                        @Html.ActionLink("View Document", "Document_View", "PublicNotice", new { FilePath = item.HindiDocumentPath, noticeid = item.publicNotiecsId, filecheck = 1 }, new { @class = "text-primary ", target = "_blank", style = "width:100px;" })
                                                                    }
                                                                    else
                                                                    {
                                                                        <p class="text-danger">No Document</p>
                                                                    }
                                                                </td>
                                                            </tr>
                                                        }
                                                    }

                                                </tbody>
